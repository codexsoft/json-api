{
    "name": "codexsoft/json-api",
    "license": "MIT",
    "description": "Tools for building JSON API backend and docs based on Symfony Forms and Swagger",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8",
        "ext-json": "*",
        "symfony/form": "^4.0 || ^5.0",
        "symfony/console": "^4.1 || ^5.0",
        "symfony/framework-bundle": "^4.3 || ^5.0",
        "symfony/validator": "^4.1 || ^5.0",
        "doctrine/collections": "^1.6",
        "codexsoft/code-arrays": "^1.0",
        "codexsoft/date-and-time": "^1.0",
        "codexsoft/code-classes": "^1.0",
        "codexsoft/code-files": "^1.0",
        "codexsoft/code-strings": "^1.0",
        "voku/stringy": "^6.2",
        "codexsoft/shortcut-tab-constant": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^6",
        "zircote/swagger-php": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "": ["src/", "vendor/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": ["tests/unit/"]
        }
    },
    "bin": [
        "bin/codexsoft.json-api"
    ]
}
